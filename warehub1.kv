<LoginScreen>:
	BoxLayout:

		orientation: 'vertical'
		padding: 30, 30, 30, 50
		spacing: 50
		size: self.size


		Widget:
			size_hint: 1, 0.2

			
			
		TextInput:
			id: username_text
			size_hint: 1, 0.18
			font_size: 40
			color: 0, 0, 1, 1
			valign: 'middle'
			halign: 'middle'
			background_color:[ 1, 1, 1, 1]
			multiline: False
			hint_text: 'ID'			
			
		TextInput:
			id: password_text
			size_hint: 1, 0.18
			font_size: 40	
	        multiline: False
	        password: True
	        hint_text: 'password'


	    Widget:

			
			
	    Button:
	    	size_hint: 1, 0.18
	    	font_size: 40
			text: 'Login'
			background_color: .88, .88, .88, 1
			on_release: root.login()




<AddUserScreen>:
	BoxLayout:

		orientation: 'vertical'
		padding: 50, 50, 50, 50
		spacing: 50
		
			
		TextInput:
			id: name_text
			size_hint: 1, 0.3
			font_size: 40
			multiline: False
			hint_text: 'name'
			
			
			
		TextInput:
			id: password_text
			size_hint: 1, 0.3
			font_size: 40	
	        multiline: False
	        password: True
	        hint_text: 'password'

	    TextInput:
			id: phone_text
			size_hint: 1, 0.3
			font_size: 40
			height: 80 		
	        multiline: False
	        hint_text: 'phone'

	    TextInput:
			id: points_text
			size_hint: 1, 0.3
			font_size: 40
			height: 80 		
	        multiline: False
	        hint_text: 'points'

	    BoxLayout:
	    	size_hint: 1, 0.2
	    	CheckBox:
	    		id: manager_radiobutton
	    		group: True
	    	Label:
	    		text: 'Manager'
	    		font_size: 30
	    	CheckBox:
	    		id: student_radiobutton
	    		group: True
	    	Label:
	    		text: 'Student'
	    		font_size: 30
	    	CheckBox:
	    		id: tech_radiobutton
	    		group: True
	    	Label:
	    		text: 'Technician'
	    		font_size: 30

	    BoxLayout:
	    	size_hint : 1, 0.2
	    	CheckBox:
	    		id: ta_checkbox
	    	Label:
	    		text: 'TA'
	    		font_size: 30

	    Widget:
	    	size_hine: 1, 0.15

	    Button:
	    	size_hint: 1, 0.3
	    	height: 80
	    	font_size: 40
			text: 'Add User'
			on_release: root.insertUser()

		Button:
	    	size_hint: 0.3, 0.3
	    	height: 80	
	    	width: 200
	    	font_size: 40
			text: 'Back'
			on_release:root.manager.current = 'clear_screen'; root.manager.current = 'manager_screen'





<ManagerScreen>:
	my_list: students_list_view
	BoxLayout:
		orientation: 'horizontal'
		id: my_boxlayout
		BoxLayout:
		
			orientation: 'vertical'
			padding: 30, 70, 30, 70
			spacing: 70

			Button:
		    	text: 'Add User'
		    	on_release:root.manager.current = 'clear_screen'; root.manager.current = 'adduser_screen'


		    Button:
		    	text: 'Get Students'
		    	on_release: root.getStudents()

		    Button:
		    	text: 'Get Technicians'
		    	on_release: root.getTechs()

		    Button:
		    	text: 'Update Information'
		    	on_release:root.manager.current = 'clear_screen'; root.manager.current = 'updateinfo_screen'

		    Spinner:
		    	text: 'Statistics'
				id: stats_spinner
				values: ['rush hours', 'crowded days', 'PCs Demands', 'ICs Demands', 'OS demands', 'Software demands', 'Complains', 'PCs Uses', 'ICs Uses']

		    Button:
		    	text: 'Sign out'
		    	on_release:root.manager.current = 'clear_screen'; root.manager.current = 'login_screen'; root.my_list.adapter.data = []

		BoxLayout:
			padding: 20, 70, 20, 20
			orientation: 'vertical'
			spacing: 20
			ListView:
		        id: students_list_view
		        adapter:
		            ListAdapter(data=[],args_converter=lambda row_index, an_obj: {'text': an_obj,'size_hint_y': None,'height': 160}, cls=main.Student, selection_mode='single', allow_empty_selection=True)

		    Button:
		    	size_hint_y: None
		    	height: 60
		    	text: 'Delete'
		    	on_release: root.deleteElement()





